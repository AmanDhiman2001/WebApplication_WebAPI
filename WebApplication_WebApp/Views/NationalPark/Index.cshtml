<div class=" row">
    <div class=" col-9">
        <h2 class=" text-black">NationalPark List</h2>
    </div>
    <div class=" col-3">
        <a asp-action="Upsert" class=" btn btn-info form-control text-black">
            <i class="fas fa-plus "></i>&nbsp; New NationalPark
        </a>
    </div>
</div>
